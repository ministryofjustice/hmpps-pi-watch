{
  "compilerOptions": {
    "incremental": true,
    "target": "es2017",
    "module": "esnext",
    "rootDir": "./src",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
